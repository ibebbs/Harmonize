<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="hostContainer" type="SimpleConfig.Section, SimpleConfig" />
    <section name="rabbitMq" type="SimpleConfig.Section, SimpleConfig" />
    <section name="eventStore" type="SimpleConfig.Section, SimpleConfig" />
  </configSections>
  <hostContainer>
    <modulePatterns>
      <modulePattern Path="Modules" Pattern="Bebbs.Harmonize.With.Messaging.Over.RabbitMq.dll" />
      <modulePattern Path="Modules" Pattern="Bebbs.Harmonize.With.Message.As.Hml.dll" />
    </modulePatterns>
  </hostContainer>
  <rabbitMq HostName="#{RabbitMqHost}" UserName="#{RabbitMqUserName}" Password="#{RabbitMqPassword}" ExchangeName="#{HarmonizeExchangeName}" QueueName="#{HarmonizeQueueName}" />
  <eventStore Host="#{EventStoreHost}" Port="#{EventStorePort}" Stream="#{EventStoreStream}" />
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive.Interfaces" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.4.0" newVersion="2.2.4.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive.Linq" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.4.0" newVersion="2.2.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>